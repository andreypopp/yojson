{
    "name": "yojson",
    "version": "1.3.3",
    "dependencies": {
        "easy-format": "andreypopp/easy-format#esy",
        "biniou": "andreypopp/biniou#esy",
        "ocamlfind": "andreypopp/ocamlfind#esy",
        "cppo": "andreypopp/cppo#esy",
        "opam-installer-bin": "andreypopp/opam-installer-bin"
    },
    "esy": {
        "build": [
            "make",
            "make install-lib",
            "(opam-installer --prefix=$cur__install || true)"
        ],
        "exportedEnv": {
            "yojson_version": {
                "scope": "global",
                "globalCollisionBehavior": "clobber",
                "val": "1.3.3"
            },
            "yojson_installed": {
                "scope": "global",
                "val": "true"
            },
            "yojson_enable": {
                "scope": "global",
                "val": "enable"
            }
        }
    },
    "peerDependencies": {
        "ocaml": "andreypopp/ocaml#esy"
    }
}
